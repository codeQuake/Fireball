		<!-- clipboard -->
		<type>
			<name>de.codequake.cms.content</name>
			<definitionname>com.woltlab.wcf.clipboardItem</definitionname>
			<listclassname>cms\data\content\ContentList</listclassname>
		</type>
		<type>
			<name>de.codequake.cms.file</name>
			<definitionname>com.woltlab.wcf.clipboardItem</definitionname>
			<listclassname>cms\data\file\FileList</listclassname>
		</type>
		<type>
			<name>de.codequake.cms.page</name>
			<definitionname>com.woltlab.wcf.clipboardItem</definitionname>
			<listclassname>cms\data\page\PageList</listclassname>
		</type>
		<type>
