			<definitionname>de.codequake.cms.content.type</definitionname>
			<classname>cms\system\content\type\TemplateContentType</classname>
			<category>code</category>
			<allowcontent>1</allowcontent>
			<allowsidebar>1</allowsidebar>
		</type>
		<type>
			<name>de.codequake.cms.content.type.group</name>
			<definitionname>de.codequake.cms.content.type</definitionname>
			<classname>cms\system\content\type\GroupContentType</classname>
			<category>structure</category>
			<allowcontent>1</allowcontent>
			<allowsidebar>1</allowsidebar>
		</type>

		<type>
			<name>de.codequake.cms.content.type.slideshow</name>
