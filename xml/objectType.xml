		<!-- /content types -->

		<!-- search -->
		<type>
			<name>de.codequake.cms.page</name>
			<definitionname>com.woltlab.wcf.searchableObjectType</definitionname>
			<classname>cms\system\search\PageSearch</classname>
		</type>
		<!-- /search -->

		<!-- Poll -->
		<type>
			<name>de.codequake.cms.content</name>
			<definitionname>com.woltlab.wcf.poll</definitionname>
			<classname>cms\system\poll\ContentPollHandler</classname>
		</type>
		<!-- /Poll -->

		<!-- user online location -->
		<type>
			<name>de.codequake.cms.page</name>
			<definitionname>com.woltlab.wcf.user.online.location</definitionname>
			<controller>cms\page\PagePage</controller>
			<classname>cms\system\user\online\location\PageLocation</classname>
			<languagevariable>cms.user.usersOnline.location.page</languagevariable>
		</type>
		<!-- /user online location -->

		<!-- acl -->
		<type>
			<name>de.codequake.cms.page</name>
			<definitionname>com.woltlab.wcf.acl</definitionname>
		</type>
		<!-- /acl -->

		<!-- comments -->
		<type>
			<name>de.codequake.cms.page.comment</name>
			<definitionname>com.woltlab.wcf.comment.commentableContent</definitionname>
			<classname>cms\system\comment\manager\PageCommentManager</classname>
		</type>
		<!-- /comments -->

		<!-- notifications -->
		<type>
			<!--
			To prevent that '\wcf\data\comment\CommentAction' fires
			the event automatically, the name was changed from
			'de.codequake.cms.page.comment.notification'. We have to
			do that manually since otherwise only the owner of the
			page gets notified, not all subscribers
			-->
			<name>de.codequake.cms.page.comment</name>
			<definitionname>com.woltlab.wcf.notification.objectType</definitionname>
			<classname>cms\system\user\notification\object\type\PageCommentUserNotificationObjectType</classname>
			<category>de.codequake.cms</category>
		</type>
		<type>
			<name>de.codequake.cms.page.comment.response.notification</name>
			<definitionname>com.woltlab.wcf.notification.objectType</definitionname>
			<classname>cms\system\user\notification\object\type\PageCommentResponseUserNotificationObjectType</classname>
			<category>de.codequake.cms</category>
		</type>
		<!-- /notifications -->

		<!-- object watch -->
		<type>
			<name>de.codequake.cms.page</name>
			<definitionname>com.woltlab.wcf.user.objectWatch</definitionname>
			<classname>cms\system\user\object\watch\PageUserObjectWatch</classname>
		</type>
		<!-- /object watch -->

		<!-- recent activity -->
		<type>
			<name>de.codequake.cms.page.comment.recentActivityEvent</name>
			<definitionname>com.woltlab.wcf.user.recentActivityEvent</definitionname>
			<classname>cms\system\user\activity\event\PageCommentUserActivityEvent</classname>
		</type>
		<type>
			<name>de.codequake.cms.page.comment.response.recentActivityEvent</name>
			<definitionname>com.woltlab.wcf.user.recentActivityEvent</definitionname>
			<classname>cms\system\user\activity\event\PageCommentResponseUserActivityEvent</classname>
		</type>
		<!-- /recent activity -->

		<!-- rebuild data worker -->
		<type>
			<name>de.codequake.cms.page</name>
			<definitionname>com.woltlab.wcf.rebuildData</definitionname>
			<classname>cms\system\worker\PageRebuildDataWorker</classname>
		</type>
		<!-- /rebuild data worker -->
	</import>
</data>
