			<category>general</category>
			<allowcontent>1</allowcontent>
			<allowsidebar>1</allowsidebar>
		</type>

		<type>
			<name>de.codequake.cms.content.type.gallery</name>
			<definitionname>de.codequake.cms.content.type</definitionname>
			<classname>cms\system\content\type\GalleryContentType</classname>
			<category>media</category>
			<allowcontent>1</allowcontent>
			<allowsidebar>0</allowsidebar>
		</type>
		<type>
			<name>de.codequake.cms.content.type.image</name>
			<definitionname>de.codequake.cms.content.type</definitionname>
			<classname>cms\system\content\type\ImageContentType</classname>
			<category>media</category>
			<allowcontent>1</allowcontent>
			<allowsidebar>1</allowsidebar>
		</type>
		<type>
			<name>de.codequake.cms.content.type.file</name>
			<definitionname>de.codequake.cms.content.type</definitionname>
			<classname>cms\system\content\type\FileContentType</classname>
			<category>media</category>
			<allowcontent>1</allowcontent>
			<allowsidebar>1</allowsidebar>
		</type>
		<type>
			<name>de.codequake.cms.content.type.youtube</name>
