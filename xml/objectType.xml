			<allowcontent>1</allowcontent>
			<allowsidebar>0</allowsidebar>
		</type>
		<type>
			<name>de.codequake.cms.content.type.fourcolumns</name>
			<definitionname>de.codequake.cms.content.type</definitionname>
			<classname>cms\system\content\type\FourColumnsContentType</classname>
