		<event>
			<name>commentResponseOwner</name>
			<objecttype>de.codequake.cms.news.comment.response.notification</objecttype>
			<classname>cms\system\user\notification\event\NewsCommentResponseOwnerUserNotificationEvent</classname>
			<preset>1</preset>
		</event>
		<!-- /news comments -->

		<!-- page comments -->
		<event>
			<name>commentResponse</name>
			<objecttype>de.codequake.cms.page.comment.response.notification</objecttype>
			<classname>cms\system\user\notification\event\PageCommentResponseUserNotificationEvent</classname>
			<preset>1</preset>
